@model WebApplication28.Models.MovieRentals 
    
@{
    ViewBag.Title = "Random";
}

<h2>@Model.movie.Name</h2>
<h2>@Model.movie.Rating</h2>
<h2>@Model.movie.ImageUrl</h2>
<ul>
@{ 
    int i = 0;
    foreach(var client in Model.clients)
    {
               <li> <a href="ShowClient/@i">@client.Name,@client.ClenskaKarta</a></li>
        i++;
    }
}
    </ul>

